<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="product" class="lt.pra_va.product.service.ProductOfService" scope="prototype" />
	<bean id="productOfService" class="lt.pra_va.product.service.ProductService" scope="prototype" />
	<bean id="productDetails" class="lt.pra_va.product.model.ProductDetails" scope="prototype"/>
	<bean id="clientProduct" class="lt.pra_va.product.model.ClientProduct" scope="prototype"/>

</beans>